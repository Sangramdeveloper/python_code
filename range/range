4  3  2  1

for j in range(4,0,-1):
	print(j,end="\t")
print()

or
r=4
for j in range(r,0,-1):
	print(j,end="\t")
print()


or
r=int(input("enter a range "))
for j in range(r,0,-1):
	print(j,end="\t")
print()


* * * *
r=int(input("enter a range "))
for j in range(1,r+1,1):
	print("*",end="\t")
print()




