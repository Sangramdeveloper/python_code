for j in range(1,5,1):
	print(j,end="\t")
print()


or
1 2 3 4
r=4
for j in range(1,r+1,1):
	print(j,end="\t")
print()

or

r=int(input("enter a range "))
for j in range(1,r+1,1):
	print(j,end="\t")
print()




4 3 2  1

